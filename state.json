[
    {
        "name": "imisdemo.imisaudit",
        "domain": "imisdemo.imisfood.com",
        "endpoint": "/imisaudit",
        "containers": [
            {
                "name": "imisdemo-imisaudit",
                "image": "gitlab.qassurance.local:5000/imisaudit-dist",
                "restart": "unless-stopped",
                "ports": [
                    "5000:80"
                ],
                "volumes": [
                    "/home/imisaudit/ApplicationData/:/app/ApplicationData"
                ],
                "env": [
                    "ASPNETCORE_URLS=http://0.0.0.0:80"
                ]
            }
        ]
    },
    {
        "name": "imisdemo.imishandbook",
        "domain": "imisdemo.imisfood.com",
        "endpoint": "/handbook",
        "containers": [
            {
                "name": "imishandbook",
                "image": "gitlab.qassurance.local:5000/imishandbook-dist",
                "restart": "unless-stopped",
                "ports": [
                    "4000:80"
                ],
                "volumes": [
                    "/home/imisaudit/ApplicationData/:/app/ApplicationData"
                ]
            }
        ]
    }
]